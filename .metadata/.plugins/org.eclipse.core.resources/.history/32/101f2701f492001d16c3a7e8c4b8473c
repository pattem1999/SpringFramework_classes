package com.SpringDemoAuthorMain.model;

import javax.persistence.Entity;

import lombok.AllArgsConstructor;
import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;

@Getter
@Setter
@AllArgsConstructor
@NoArgsConstructor
@Entity
public class Author {

	private Integer id;
	
	private String name;
	
	private String country;
	
	
	
}
